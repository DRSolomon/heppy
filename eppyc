#!/usr/bin/env python

import Net
import sys
import socket

class Client:
    def __init__(self, address):
        self.socket = socket.socket(socket.AF_UNIX, socket.SOCK_STREAM)
        self.socket.settimeout(0.01)
        self.socket.connect(address)
        self.socket.settimeout(None)

    def write(self, data):
        Net.write(self.socket, data)

    def read(self):
        return Net.read(self.socket)

    def command(self, data):
        self.write(data)
        return self.read()

if len(sys.argv)<2:
    print('usage: eppyc path')
    exit(1)
path = sys.argv[1]

data = ''
for line in sys.stdin:
    data += line

if not data:
    exit(1)

try:
    client = Client(path)
    res = client.command(data)
except:
    res = None

print res
